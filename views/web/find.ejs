                                            <h6 class="card-title ten"><%= books.name %></h6>
                                            <small class="tacgia text-muted"><%= books.author %></small>
                                            <div class="gia d-flex align-items-baseline">
                                                <div class="giamoi"><%= books.price %> ₫</div>
                                                <div class="giacu text-muted"><%= books.cost %> ₫</div>
                                                <div class="sale">-<%= books.discount %>%</div>
                                            </div>
